&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt;&lt;span style="color:#57606a"&gt;// let sum_low = nums.iter().filter(|&amp;amp;&amp;amp;x| x &amp;lt;= key).sum::&amp;lt;i32&amp;gt;();
&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt;&lt;span style="color:#57606a"&gt;// let one = key * (key + 1) / 2 - sum_low;
