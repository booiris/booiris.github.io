&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt; &lt;span style="color:#1f2328"&gt;}&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt; &lt;span style="color:#cf222e"&gt;int&lt;/span&gt; res &lt;span style="color:#0550ae"&gt;=&lt;/span&gt; INT_MAX&lt;span style="color:#1f2328"&gt;;&lt;/span&gt;
