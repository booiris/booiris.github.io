&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt; res &lt;span style="color:#0550ae"&gt;=&lt;/span&gt; min&lt;span style="color:#1f2328"&gt;(&lt;/span&gt;res&lt;span style="color:#1f2328"&gt;,&lt;/span&gt; key1&lt;span style="color:#1f2328"&gt;[&lt;/span&gt;i&lt;span style="color:#1f2328"&gt;]&lt;/span&gt; &lt;span style="color:#0550ae"&gt;+&lt;/span&gt; cnt0 &lt;span style="color:#0550ae"&gt;-&lt;/span&gt; key0&lt;span style="color:#1f2328"&gt;[&lt;/span&gt;i&lt;span style="color:#1f2328"&gt;]);&lt;/span&gt;
