&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt; &lt;span style="color:#cf222e"&gt;while&lt;/span&gt; &lt;span style="color:#1f2328"&gt;(&lt;/span&gt;high &lt;span style="color:#0550ae"&gt;&amp;lt;&lt;/span&gt; n &lt;span style="color:#0550ae"&gt;&amp;amp;&amp;amp;&lt;/span&gt; now &lt;span style="color:#0550ae"&gt;*&lt;/span&gt; nums&lt;span style="color:#1f2328"&gt;[&lt;/span&gt;high&lt;span style="color:#1f2328"&gt;]&lt;/span&gt; &lt;span style="color:#0550ae"&gt;&amp;lt;&lt;/span&gt; k&lt;span style="color:#1f2328"&gt;)&lt;/span&gt; &lt;span style="color:#1f2328"&gt;{&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt; now &lt;span style="color:#0550ae"&gt;*=&lt;/span&gt; nums&lt;span style="color:#1f2328"&gt;[&lt;/span&gt;high&lt;span style="color:#0550ae"&gt;++&lt;/span&gt;&lt;span style="color:#1f2328"&gt;];&lt;/span&gt;
